[user]
    name = Ryan McQuen
    email = rmcquen@funko.com
[core]
    excludesfile = /Users/rmcquen/.gitignore_global
[merge]
    tool = vscode
[mergetool "vscode"]
    cmd = "code --wait $MERGED"
[diff]
    tool = smerge
[difftool "vscode"]
    cmd = "code --wait --diff $LOCAL $REMOTE"
[init]
    templatedir = ~/.git-templates
[pager]
    branch = false
[mergetool "smerge"]
    cmd = smerge mergetool \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
    trustExitCode = true
[difftool "smerge"]
    cmd = smerge \"$BASE\" \"$LOCAL\" \"$REMOTE\"
